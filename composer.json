{
    "name": "pug-php/pug-filter-coffee-script",
    "replace": {
        "kylekatarnls/jade-filter-coffee-script": "self.version"
    },
    "description": "Render CoffeeScript code as JavaScript in a script tag",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Katarn",
            "email": "kylekatarnls@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "pug-php/pug": "^2.0.1 || ^3.0.0",
        "nodejs-php-fallback/coffeescript": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 <6.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Pug": ["src/", "alias/"]
        }
    }
}
